[receive]
	requireContributorAgreement = true
[submit]
	action = merge always
	mergeContent = true
[access]
	inheritFrom = Platform-Unrestricted-Projects
[access "refs/*"]
	push = group mdb/copybara-piper
	forgeAuthor = group mdb/copybara-piper
	forgeCommitter = group mdb/copybara-piper
[access "refs/heads/*"]
	label-Code-Review = -2..+2 group mdb/copybara-piper
	label-Code-Review = -2..+2 group mdb/rtc-dialer-eng
	label-Verified = -1..+1 group mdb/copybara-piper
	label-Verified = -1..+1 group mdb/rtc-dialer-eng
	editAssignee = group mdb/rtc-dialer-eng
	push = group mdb/copybara-piper
